{{setLevel this 0~}}
{{#if (descendants min=(option 'memberIndex.minDescendants'))~}}
  {{#if isExported~}}
    {{#if (equal (depth) 0)}}
      {{#if (optionEquals "member-index-format" "list")}}
        <ul class="memberindex-list-0">
          {{>member-index-list}}
        </ul>
      {{else if (optionEquals "member-index-format" "toc")~}}
        [
          {{~>member-index-toc~}}
        ]
      {{~else~}}
        <ul class="member-index-grouped-0">
          {{>member-index-grouped}}
        </ul>
      {{/if~}}
    {{/if~}}
  {{else~}}
    {{#if (optionEquals "member-index-format" "list")}}
      <ul class="member-index-list nav toc">
        {{>member-index-list}}
      </ul>
    {{else if (optionEquals "member-index-format" "toc")~}}
      {{#if @first }}[{{/if}}
        {{~>member-index-toc~}}
      {{#if @last}}]{{/if}}
    {{~else}}
      <ul class="member-index-grouped">
        {{>member-index-grouped}}
      </ul>
    {{/if~}}
  {{/if~}}
{{/if}}
