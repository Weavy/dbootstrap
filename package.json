{
  "name": "dbootstrap",
  "author": "Weavy",
  "version": "4.0.1",
  "description": "Bootstrap HTML output template for jsdoc2md",
  "license": "MIT",
  "repository": "https://github.com/Weavy/dbootstrap.git",
  "main": "./lib/dbs.js",
  "keywords": [
    "documentation",
    "bootstrap"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "files": [
    "helpers",
    "lib",
    "partials"
  ],
  "scripts": {
    "test": "test-runner test/*.js test/ddata/*.js",
    "docs": "jsdoc2md -t jsdoc2md/README.hbs -p list -l js lib/*.js > README.md; echo"
  },
  "dependencies": {
    "array-back": "^2.0.0",
    "cache-point": "^0.4.1",
    "common-sequence": "^1.0.2",
    "file-set": "^2.0.0",
    "handlebars": "^4.1.1",
    "marked": "^0.6.2",
    "object-get": "^2.1.0",
    "reduce-flatten": "^1.0.1",
    "reduce-unique": "^1.0.0",
    "reduce-without": "^1.0.1",
    "test-value": "^3.0.0",
    "walk-back": "^3.0.0"
  },
  "devDependencies": {
    "dmd-plugin-example": "^0.1.0",
    "test-runner": "^0.5.0"
  },
  "standard": {
    "ignore": [
      "example",
      "test/fixture",
      "test/overrides"
    ]
  },
  "peerDependencies": {
    "jsdoc-parse": "^3.0.1"
  }
}
